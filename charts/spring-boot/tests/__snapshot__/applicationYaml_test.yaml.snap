injects application properties spring-container:
  1: |
    containers:
      - env:
          - name: SPRING_PROFILES_ACTIVE
            value: profile1, profile2, profile3
        image: :1.16.0
        imagePullPolicy: IfNotPresent
        livenessProbe:
          httpGet:
            path: /
            port: http
        name: spring-boot
        ports:
          - containerPort: 80
            name: http
            protocol: TCP
        readinessProbe:
          httpGet:
            path: /
            port: http
        resources: {}
        securityContext: {}
        volumeMounts:
          - mountPath: /app/config/helm-config
            name: helm-application-properties
          - mountPath: /app/config/config-map/application-profile3.properties
            name: user-application-properties-profile3
            subPath: my-key
    securityContext: {}
    serviceAccountName: RELEASE-NAME-spring-boot
    volumes:
      - configMap:
          name: RELEASE-NAME-spring-boot-application-properties
        name: helm-application-properties
      - configMap:
          name: user-defined-config-map
        name: user-application-properties-profile3
  2: |
    apiVersion: v1
    data:
      application-profile1.yaml: |
        server:
          port: 8080
      application-profile2.yaml: |
        logging:
          level:
            org.springframework: error
            com.example.myapp: debug
    kind: ConfigMap
    metadata:
      name: RELEASE-NAME-spring-boot-application-properties
mounts configuration values:
  1: |
    containers:
      - env:
          - name: SPRING_PROFILES_ACTIVE
            value: userConfig
        image: :1.16.0
        imagePullPolicy: IfNotPresent
        livenessProbe:
          httpGet:
            path: /
            port: http
        name: spring-boot
        ports:
          - containerPort: 80
            name: http
            protocol: TCP
        readinessProbe:
          httpGet:
            path: /
            port: http
        resources: {}
        securityContext: {}
        volumeMounts:
          - mountPath: /app/config/helm-config
            name: helm-application-properties
    securityContext: {}
    serviceAccountName: RELEASE-NAME-spring-boot
    volumes:
      - configMap:
          name: RELEASE-NAME-spring-boot-application-properties
        name: helm-application-properties
mounts referenced configMaps:
  1: |
    containers:
      - env:
          - name: SPRING_PROFILES_ACTIVE
            value: userConfig
        image: :1.16.0
        imagePullPolicy: IfNotPresent
        livenessProbe:
          httpGet:
            path: /
            port: http
        name: spring-boot
        ports:
          - containerPort: 80
            name: http
            protocol: TCP
        readinessProbe:
          httpGet:
            path: /
            port: http
        resources: {}
        securityContext: {}
        volumeMounts:
          - mountPath: /app/config/helm-config
            name: helm-application-properties
          - mountPath: /app/config/config-map/application-userConfig.yaml
            name: user-application-properties-userConfig
            subPath: application.yaml
    securityContext: {}
    serviceAccountName: RELEASE-NAME-spring-boot
    volumes:
      - configMap:
          name: RELEASE-NAME-spring-boot-application-properties
        name: helm-application-properties
      - configMap:
          name: user-defined-config-map
        name: user-application-properties-userConfig
